# Google Tag Manager container ID (injected into index.html at build time).
# Set for production; leave unset or GTM-XXXXXX for dev if you don't want GTM to load.
VITE_GTM_ID=

# EmailJS (contact form). Required for form submission.
VITE_EMAILJS_SERVICE_ID=
VITE_EMAILJS_TEMPLATE_ID=
VITE_EMAILJS_PUBLIC_KEY=
